{
  "name": "compressed-log-viewer",
  "displayName": "Compressed Log Viewer",
  "description": "View compressed log files (XZ, bzip2) directly in VS Code",
  "version": "1.0.1",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "compressed-log-viewer.compressed",
        "displayName": "Compressed File Viewer",
        "selector": [
          {
            "filenamePattern": "*.xz"
          },
          {
            "filenamePattern": "*.tar.xz"
          },
          {
            "filenamePattern": "*.bz2"
          },
          {
            "filenamePattern": "*.tar.bz2"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "TS_NODE_PROJECT=test/tsconfig.json mocha --require ts-node/register 'test/unit/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "chai": "^4.3.10",
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "lzma-native": "^8.0.6",
    "seek-bzip": "^2.0.0",
    "tar-stream": "^3.1.7"
  },
  "publisher": "hankhsu1996",
  "repository": {
    "type": "git",
    "url": "https://github.com/hankhsu1996/compressed-log-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/hankhsu1996/compressed-log-viewer/issues"
  },
  "homepage": "https://github.com/hankhsu1996/compressed-log-viewer",
  "license": "MIT",
  "author": {
    "name": "Shou-Li Hsu",
    "email": "hank850503@gmail.com"
  }
}
